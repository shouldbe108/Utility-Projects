import random

#upper case characters
charu="QWERTYUIOPASDFHJKLZXCVBNM"
#lower case cahracters
charl="qwertyuiopsdfghjklzxcvbnm"
#numbers
numb="1234567890"
#symbols 
symb="!@#$%^&*()[];<>"

#empty string for password var
password=""

#for loop to create 16 char password (4 chars per loop)
for i in range(0,4):
    a = (random.choice(charu))
    for j in range (0,4):
        b = (random.choice(charl))
        for k in range (0,4):
            c = (random.choice(numb))
            for l in range(0,4):
                d = (random.choice(symb))
            
    password = str(password)+str(a)+str(b)+str(c)+str(d)

#resulting password
print(password)
        
        
    
